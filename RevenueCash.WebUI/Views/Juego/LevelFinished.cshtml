@model RevenueCash.Models.Juego.Game
@{
    ViewBag.Title = "LevelFinished";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="levelFinished">NIVEL FINALIZADO</div>
@if (Model.CurrentLevel.NextLevel != null)
{
    <div class="container">
        <div class="btn">
            <span class="nextLevel">@Html.ActionLink("Siguiente nivel", "NextLevel")</span>
            <div class="dot"></div>
        </div>
    </div>
}

